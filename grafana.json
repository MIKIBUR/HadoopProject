          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH RankedRatings AS (\r\n    SELECT\r\n        politician,\r\n        rating,\r\n        ROW_NUMBER() OVER (PARTITION BY politician ORDER BY timestamp DESC) AS row_num\r\n    FROM\r\n        politicians\r\n    WHERE\r\n        politician IN ('trump', 'clinton', 'obama')\r\n)\r\nSELECT\r\n    politician,\r\n    AVG(rating) AS average_rating\r\nFROM\r\n    RankedRatings\r\nWHERE\r\n    row_num <= 50\r\nGROUP BY\r\n    politician;\r\n",
          "refId": "A",
          "sql": {
